{
  "agents": {
    "prometheus": {
      "enabled": true,
      "replace_plan": true
    },
    "librarian": {
      "enabled": true,
      "model": "google/gemini-3-flash-preview"
    },
    "explore": {
      "enabled": true,
      "model": "google/gemini-3-flash-preview"
    },
    "oracle": {
      "model": "google/gemini-3-flash-preview"
    }
  },
  "disabled_hooks": [
    "comment-checker",
    "startup-toast"
  ],
  "disabled_mcps": [],
  "lsp": {
    "typescript-language-server": {
      "command": [
        "npx",
        "typescript-language-server",
        "--stdio"
      ],
      "extensions": [
        ".ts",
        ".tsx"
      ],
      "priority": 10
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "google/gemini-3-flash-preview"
    },
    "artistry": {
      "model": "google/gemini-3-flash-preview"
    },
    "deep": {
      "model": "google/gemini-3-flash-preview"
    },
    "quick": {
      "model": "google/gemini-3-flash-preview"
    },
    "writing": {
      "model": "google/gemini-3-flash-preview"
    }
  },
  "experimental": {
    "preemptive_compaction_threshold": 0.85,
    "truncate_all_tool_outputs": true,
    "auto_resume": true
  }
}
